{
  "name": "vmeretail/laravel-broadway",
  "description": "A Laravel adapter for the Broadway ES/CQRS package. VME version",
  "license": "MIT",
  "keywords": [
    "laravel",
    "broadway",
    "event sourcing",
    "ES",
    "CQRS",
    "Domain Driven Design",
    "DDD"
  ],
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "doctrine/dbal": "~2.5",
    "broadway/broadway": "~3.0.0",
    "broadway/event-store-dbal": "^0.2.0",
    "illuminate/support": "~5.5|~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0"
  },
  "autoload": {
    "psr-4": {
      "Nwidart\\LaravelBroadway\\": "src/"
    }
  },
  "suggest": {
    "broadway/read-model-elasticsearch": "For persisting read models in elasticsearch"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Nwidart\\LaravelBroadway\\LaravelBroadwayServiceProvider"
      ]
    }
  }
}
